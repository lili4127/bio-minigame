<CellScreen>:
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        Button:
            text: 'Main Menu'
            on_press: root.manager.current = 'menu'
            size_hint: .3, .3
    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'top'
        Button:
            text: 'Reset'
            on_press: root.manager.current = 'menu'
            size_hint: .3, .3
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'top'
        Label:
            text: "00:00.00"
            size_hint: .3, .3
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'top'
        Label:
            text: root.cur_label
            size_hint: .5, .5
    FloatLayout:
        CellButton:
            source: root.cur_path
            pos_hint: {"x":.3,"y":.3}
            background_normal: ''
            size_hint: .5, .5
